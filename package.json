{
  "name": "message-center.js",
  "version": "2.0.1",
  "description": "MessageCenter",
  "main": "lib/MessageCenter.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "mkdir -p lib && babel ./src -d ./lib",
    "copy": "mkdir -p build && cp -R src lib package.json index.d.ts README.md build",
    "publish2npm": "npm run build && npm run copy && npm publish ./build",
    "clean": "rm -rf ./lib",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanyuantingfeng/nmessage-center.git"
  },
  "keywords": [
    "message",
    "center",
    "messagecenter",
    "message-center"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.2"
  },
  "author": "nanyuantingfeng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nanyuantingfeng/nmessage-center/issues"
  },
