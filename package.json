{
  "name": "nmessage-center",
  "version": "2.0.0",
  "description": "Message Center",
  "main": "lib/MessageCenter.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "mkdir -p lib && babel ./src -d ./lib",
    "copy": "mkdir -p build && cp -R src lib package.json index.d.ts README.md build",
    "publish2npm": "npm run build && npm run copy && npm publish ./build",
    "clean": "rm -rf ./lib",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanyuantingfeng/nmessage-center.git"
  },
  "keywords": [
    "message",
    "center"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers-2": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-undefined-to-void": "^6.8.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-ie": "^6.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-jest": "^20.0.3",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1"
  },
  "author": "nanyuantingfeng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nanyuantingfeng/nmessage-center/issues"
  },
  "homepage": "https://github.com/nanyuantingfeng/nmessage-center#readme"
}
